{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": "iam:*",
      "Resource": "*"
    }
  ]
}
cat > mypolicy.json

aws iam put-user-policy --user-name devv1 --policy-name MyPolicy --policy-document mypolicy.json
aws iam put-user-policy --user-name devs1 --policy-name MyPowerUserRole --policy-document file://mypolicy1.json

